{
    "name": "danmcadams/mandrill-sdk-wrapper",
    "description": "A wrapper for the Mandrill API PHP SDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan McAdams",
            "email": "danmcadams@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DZMC\\Mandrill\\": "src/Mandrill"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DZMC\\Mandrill\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.0",
        "ext-curl": "*",
        "psr/log": "^1.0.1",
        "mandrill/mandrill": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "monolog/monolog": "^1.23"
    }
}
